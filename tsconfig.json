{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "ES2018",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "strict": true,
    "baseUrl": "./src", 
    "paths": {
      "App/*": ["App/*"],
      "App": ["App"],
      "Pages/*": ["Pages/*"],
      "Pages": ["Pages"],
      "Containers/*": ["Containers/*"],
      "Containers": ["Containers"],
      "Components/*": ["Components/*"],
      "Components": ["Components"],
      "assets/*": ["assets/*"],
      "assets": ["assets"],
      "helpers/*": ["helpers/*"],
      "helpers": ["helpers"],
      "hooks/*": ["hooks/*"],
      "hooks": ["hooks"],
      "services/*": ["services/*"],
      "services": ["services"],
      "store/*": ["store/*"],
      "store": ["store"],
      "utils/*": ["utils/*"],
      "utils": ["utils"]
    },
    "plugins": []
  }
}